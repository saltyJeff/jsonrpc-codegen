{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "JSONRPC-Codegen schema",
	"type": "object",
	"properties": {
		"include": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"description": "Schema files to include"
		},
		"outDir": {
			"type": "string",
			"description": "Directory to export interfaces to"
		},
		"abstractClass": {
			"type": "boolean",
			"default": false,
			"description": "If set, an abstract class will be generated instead of an enum"
		}
	},
	"required": [
		"include",
		"outDir"
	]
}