{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "JSONRPC-Codegen method",
	"type": "object",
	"properties": {
		"name": {
			"type": "string"
		},
		"endpointName": {
			"type": "string",
			"description": "Name of the endpoint and interface"
		},
		"typesOnly": {
			"type": "boolean",
			"default": false
		},
		"params": {
			"$ref": "http://json-schema.org/draft-07/schema#",
			"type": "object",
			"description": "Input parameters"
		},
		"result": {
			"$ref": "http://json-schema.org/draft-07/schema#",
			"type": "object",
			"description": "Result data"
		}
	}
}